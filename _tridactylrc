set searchurls.t       https://www.thepiratebay.org/search/%s/0/99/0
set searchurls.v       https://torrents-csv.ml/#/search/torrent/%s/1
set searchurls.r       https://old.reddit.com/r/%s
set searchurls.u       https://old.reddit.com/u/%s
set searchurls.w       https://en.wikipedia.org/w/index.php?search=%s
set searchurls.g       https://www.google.com/search?q=%s
set searchurls.s       https://duckduckgo.com/lite/?q=%s
set searchurls.wd      https://en.wiktionary.org/wiki/?search=%s
set searchurls.d       https://www.websters1913.com/words/%s
set searchurls.caniuse https://caniuse.com/#search=%s

set searchengine g

set editorcmd gvim -f "+set ft=markdown tw=72 spell"

